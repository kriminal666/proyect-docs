;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	server10.iesebre.com. root.iesebre.com. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	server10.
192.168.202.1	IN	PTR	server10
192.168.203.1	IN	PTR	server10
192.168.204.1	IN	PTR	server10
192.168.202.1	IN	PTR	iesebre.com
192.168.203.1	IN	PTR	iesebre.com
192.168.204.1	IN	PTR	iesebre.com
